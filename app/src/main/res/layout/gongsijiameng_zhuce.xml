                    android:layout_height="60dp"
                    android:gravity="center"
                    android:paddingTop="9dp"
                    android:paddingLeft="7dp"
                    android:src="@drawable/xiangji"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="43dp" />

                <EditText
                    android:id="@+id/scxm_sendmessage"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/login_edit_normal"
                    android:singleLine="true"
                    android:layout_toEndOf="@+id/btn_sendxm" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/imageView2"
                    android:src="@drawable/touxian"
                    android:layout_centerVertical="true"
                    android:layout_alignEnd="@+id/scxm_sendmessage"
                    android:layout_toEndOf="@+id/btn_sendxm" />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/fzrsj_bottom2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true">
                <TextView
                    android:id="@+id/btn_send6"
                    android:layout_width="120dp"
                    android:layout_height="50dp"
                    android:gravity="right"
                    android:paddingTop="9dp"
                    android:textColor="#000"

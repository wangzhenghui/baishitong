                    android:layout_toEndOf="@+id/btn_send0" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/sqxm_bottom2"
                android:layout_width="match_parent"
                android:layout_height="134dp"
                android:layout_alignParentBottom="true">
                <ImageView

                    android:layout_alignTop="@+id/et_sendmessage6"
                    android:layout_alignParentStart="true" />

                <EditText
                    android:id="@+id/et_sendmessage6"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/login_edit_normal"
                    android:singleLine="true"
                    android:hint="此号码将作为登录账号"
                    android:textSize="14dp"
                    android:layout_alignParentBottom="true"
                    android:layout_toEndOf="@+id/btn_send6" />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/yzm_bottom2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true">
                <TextView
                    android:id="@+id/btn_send7"
                    android:layout_width="120dp"
                    android:layout_height="50dp"
                    android:gravity="right"
                    android:paddingTop="9dp"
                    android:textColor="#000"
                    android:text="验证码："
                    android:layout_alignTop="@+id/et_sendmessage7"

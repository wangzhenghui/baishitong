            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/zzhm_bottom2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true">
                <TextView
                    android:id="@+id/btn_send0"
                    android:layout_width="120dp"

                    android:paddingTop="9dp"
                    android:paddingLeft="7dp"
                    android:textColor="#000"
                    android:text="联系电话："
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true" />

                <EditText
                    android:id="@+id/et_sendmessage3"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/login_edit_normal"
                    android:singleLine="true"
                    android:hint="手机号码或固定电话"
                    android:textSize="14dp"
                    android:layout_alignParentBottom="true"
                    android:layout_toEndOf="@+id/btn_send3" />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/xb_bottom2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true">
                <TextView
                    android:id="@+id/btn_send4"
                    android:layout_width="120dp"
                    android:layout_height="40dp"
                    android:gravity="right"
                    android:paddingTop="9dp"
                    android:paddingLeft="7dp"
                    android:textColor="#000"
                    android:text="公司地址："
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true" />

                <EditText
                    android:id="@+id/et_sendmessage4"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/login_edit_normal"
                    android:singleLine="true"
                    android:hint="公司地址"
                    android:textSize="14dp"
                    android:layout_alignParentBottom="true"
                    android:layout_toEndOf="@+id/btn_send4" />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/dz_bottom2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true">
                <TextView
                    android:id="@+id/btn_send5"
                    android:layout_width="120dp"
                    android:layout_height="40dp"
                    android:gravity="right"
                    android:paddingTop="9dp"
                    android:paddingLeft="7dp"
                    android:textColor="#000"
                    android:text="负责人："
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true" />

                <EditText
                    android:id="@+id/et_sendmessage5"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/login_edit_normal"
                    android:singleLine="true"
                    android:hint="请输入公司负责人"
                    android:textSize="14dp"
                    android:layout_alignParentBottom="true"
                    android:layout_toEndOf="@+id/btn_send5" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/yh_bottom2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true">

<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eee"
    android:weightSum="1">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:id="@+id/title"
        android:background="@drawable/lan_bar"
        android:gravity="center_vertical"  >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="商户注册"
            android:textSize="20dp"
            android:textColor="#0b0b0b"
            android:id="@+id/textView"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true" />
        <TextView
            android:id="@+id/zhuce_reback_btn"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:text="2/2"
            android:textSize="18dp"
            android:textColor="#060606"
            android:layout_alignBottom="@+id/textView"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="119dp"
        android:id="@+id/title1"
        android:background="@drawable/lan_bar"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="matrix"
            android:src="@drawable/touxian"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="公司加盟申请"
            android:textSize="25dp"
            android:textColor="#ece40c"
            android:id="@+id/textView4"
            android:layout_below="@+id/imageView"
            android:layout_centerHorizontal="true" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:id="@+id/title3"
        android:background="@drawable/lan_bar"
        android:gravity="center_vertical"  >

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="39dp"
        android:id="@+id/title21"
        android:background="@drawable/lan_bar"
        android:gravity="center_vertical"  >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="请填写以下资料"
            android:textSize="20dp"
            android:textColor="#f4ec0b"
            android:id="@+id/textView2"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true" />

    </RelativeLayout>

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbarStyle="outsideOverlay"
        android:fillViewport="false">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/xm_bottom2"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_weight="0.09">
                <TextView
                    android:id="@+id/btn_send2"
                    android:layout_width="120dp"
                    android:layout_height="40dp"
                    android:gravity="right"
                    android:paddingTop="9dp"
                    android:paddingLeft="7dp"
                    android:textColor="#000"
                    android:text="公司名称："
                    android:layout_alignTop="@+id/et_sendmessage2"
                    android:layout_alignParentStart="true" />

                <EditText
                    android:id="@+id/et_sendmessage2"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/login_edit_normal"
                    android:singleLine="true"
                    android:hint="请输入公司名称"
                    android:textSize="14dp"
                    android:layout_alignParentBottom="true"
                    android:layout_toEndOf="@+id/btn_send2" />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/sf_bottom2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
